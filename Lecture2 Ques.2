#include <stdio.h>

int main()
{
    double a, b, c, d, m, r;
    printf("Enter a Number :  ", a);
    scanf("%lf", &a);

    printf("Enter a Number :  ", b);
    scanf("%lf", &b);

    c = a + b;
    printf("%lf\n", c);

    d = a - b;
    printf("%lf\n", d);

    m = a * b;
    printf("%lf\n", m);

    r = a / b;
    printf("%lf\n", r);
    
    return 0;
}
