#include <stdio.h>
#include <stdbool.h>

int main()
{
    char a = 'C';                   
    char b[] = "Bro";               

    float c = 3.141592;            
    double d = 3.141592653589793;     

    bool e = true;                  

    char f = 100;                  

    int j = 2147483647;            
    

    printf("%c\n", a);  // char
    printf("%s\n", b);  // character array
    printf("%f\n", c);  // float
    printf("%0.10lf\n", d); // double
    printf("%d\n", e);  // bool
    printf("%d\n", f);  // char as numeric value
    printf("%d\n", j);  // int
    return 0;
}
